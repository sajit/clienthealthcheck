{
    "name": "Client Health Checker",
    "version": "0.0.2",
    "manifest_version": 2,
    "permissions": [ "https://*/"],
    "background": {
        "scripts": ["js/vendor/angular.min.js",
            "js/vendor/angular-route.min.js","js/background/background.js"]
    },
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "icons" : {"128" : "icons/icon-128-m.png"},
    "app" : {
        "launch" : {
            "local_path" : "tab.html"
        }
    }

}